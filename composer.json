{
    "name": "laravel-tools/laravel-git",
    "description": "A Laravel package for running Git commands",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Scott",
            "email": "codetheorist@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "LaravelTools\\LaravelGit\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
          "LaravelTools\\LaravelGit\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTools\\LaravelGit\\LaravelGitServiceProvider"
            ],
            "aliases": {
                "Git": "LaravelTools\\LaravelGit\\Facades\\Git"
            }
        }
    },
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "orchestra/testbench": "3.5.*"      
    }
}
